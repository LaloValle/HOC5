# Codigo 1
i = 0;
while(i <= 10){
	if(i % 2 == 0)
		print('Par');
	else
		print('impar');
	i++;
}

i = 0;while(i <= 10){if(i % 2 == 0)print('Par');else print('impar');i++;}

#Codigo2
i = 0;
j = 0;
while(i < 2){
	while(j < 4){
		if(i*j% 5 == 0) print(i*j)
	}
	i = i+1;
}

i = 0;j = 0;while(i <= 2){ while(j <= 2){	if((i*j) % 2 == 0) print(i*j); j = j+1;	} i = i+1;}

# Codigo 3
for(i=0;i<10;i++){if(i % 2 == 0)print('Par');else print(i);} print('Prueba del for\n');